{
  "name": "gameOfLife",
  "version": "0.0.1",
  "description": "Conway's game of life implementation",
  "main": "lib/entry.js",
  "scripts": {
    "start": "jspm install && npm run server",
    "build-js": "jspm bundle-sfx src/entry dist/bundle.js && jspm bundle-sfx src/entry dist/bundle.min.js --minify",
    "build-js:stable": "jspm bundle-sfx src/stable/entry dist/bundle.js",
    "watch-js": "watch 'npm run build-js:stable' src",
    "test": "testem",
    "server": "http-server"
  },
  "repository": "",
  "author": "Zuri Pabon",
  "license": "",
  "dependencies": {},
  "devDependencies": {
    "http-server": "^0.8.0",
    "jspm": "^0.15.6",
    "testem": "^0.8.3",
    "watch": "^0.16.0"
  },
  "jspm": {
    "directories": {},
    "dependencies": {
      "jquery": "github:components/jquery@^2.1.3",
      "lodash": "npm:lodash@^3.8.0",
      "should": "npm:should@^6.0.1"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.88",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.88"
    }
  }
}